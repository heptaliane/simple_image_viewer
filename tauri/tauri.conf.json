{
  "build": {
    "beforeDevCommand": "cd yew && trunk serve",
    "beforeBuildCommand": "cd yew && trunk build",
    "devPath": "http://localhost:1420",
    "distDir": "../yew/dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "simple_image_viewer",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "heptaliane.simple-image-viewer",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "cli": {
      "args": [
        {
          "name": "filename",
          "takesValue": true,
          "index": 1
        }
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "simple_image_viewer",
        "width": 800,
        "height": 600
      }
    ]
  }
}
